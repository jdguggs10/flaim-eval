# OpenAI API key (required)
OPENAI_API_KEY=sk-...

# OAuth credentials (populated by bootstrap)
FLAIM_CLIENT_ID=
FLAIM_REFRESH_TOKEN=

# Target MCP server (default: production)
FLAIM_MCP_URL=https://api.flaim.app/mcp

# Auth endpoints
FLAIM_AUTH_BASE_URL=https://api.flaim.app

# Model override (default: gpt-5-mini-2025-08-07)
# FLAIM_EVAL_MODEL=gpt-5-mini-2025-08-07

# Cloudflare Workers Observability (optional â€” enables server-side log enrichment)
# Token should be scoped for Workers observability query access on your account.
# CLOUDFLARE_ACCOUNT_ID=
# CLOUDFLARE_API_TOKEN=

# Trace filtering + re-enrichment tuning
# Strict isolation default: leave unset/0. Set to 1 only for legacy run-level fallback debugging.
# FLAIM_EVAL_ALLOW_RUN_FALLBACK=0
# FLAIM_EVAL_REENRICH_ATTEMPTS=6
# FLAIM_EVAL_REENRICH_DELAY_MS=15000
# FLAIM_EVAL_REENRICH_WINDOW_EXPAND_MS=30000
